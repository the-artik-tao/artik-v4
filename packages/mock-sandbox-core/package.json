{
  "name": "@the-artik-tao/mock-sandbox-core",
  "version": "0.1.0",
  "type": "module",
  "description": "Library-first toolkit for discovering API calls in React apps, synthesizing mocks via DMR, and running sandboxed environments",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest"
  },
  "keywords": [
    "mock",
    "sandbox",
    "react",
    "api",
    "testing",
    "dmr",
    "docker"
  ],
  "dependencies": {
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "glob": "^10.3.10",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
